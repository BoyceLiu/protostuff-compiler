delimiters "%", "%"
import "io/protostuff/compiler/core.stg"

type_link(type) ::= <%
<a href="%type:type_href()%">%type.name%</a>
%>

type_href(type) ::= <%
%if(is_scalar.(type.fullyQualifiedName))%
https://developers.google.com/protocol-buffers/docs/proto#scalar
%else%
#page=%type.canonicalName%
%endif%
%>
